    "image-webpack-loader": "~4.2.0",
    "jest": "~23.1.0",
    "lint-staged": "7.0.0",
    "mobx-react-devtools": "~5.0.1",
    "node-sass": "~4.7.2",
    "prettier": "1.11.1",
    "raw-loader": "^0.5.1",
    "react-unit": "~3.0.3",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "~6.0.7",
    "skin-deep": "~1.2.0",
    "style-loader": "~0.20.3",
    "url-loader": "~1.0.1",
